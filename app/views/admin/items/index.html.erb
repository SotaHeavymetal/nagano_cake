
 <h2>商品一覧</h2>
 </div>


 <%= link_to "+", new_admin_item_path, class: "btn shadow-sm" %>
 </div>
<table class = "table text-center">

  <thead>
    <tr>
      <th>商品ID</th>
      <th>商品名</th>
      <th>税抜価格</th>
    </tr>
  </thead>

  <tbody>
    <% @items.each do |item| %>
    <tr>
      <td><%= item.id %></td>
      <td><%= link_to item.name, admin_item_path(item) %></td>
      <td><%= item.price %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<!--%= paginate @items %>-->