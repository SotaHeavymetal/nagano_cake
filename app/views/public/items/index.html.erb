<div class ="container md-5">
  <div class = "row">
    <h3><strong>商品一覧</strong><h5>(全x件)</5></h3>
    </div>
  <div class = "row">
    <div class = "col-md-3">
      <%= @items.each do |item| %>
      <%= attachment_image_tag item, :image, size: "200x200", fallback: 'no_image.jpeg' %>
      <div><%= item.name %></div>
      <div><%= (item.price * 1.1).floor %></div>
      <% end %>
    </div>
     <%#= paginate @items %>
  </div>